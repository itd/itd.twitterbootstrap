<?xml version="1.0"?>
<object name="portal_css">
  <stylesheet
    id="++theme++itd.twitterbootstrap/assets/local-static.css"
    expression="request/HTTP_X_THEME_ENABLED | nothing"
    applyPrefix="True" />

  <!-- hide public.css if the theme is enabled -->
  <stylesheet
    id="public.css"
    expression="not: request/HTTP_X_THEME_ENABLED | nothing"
    />
</object>
